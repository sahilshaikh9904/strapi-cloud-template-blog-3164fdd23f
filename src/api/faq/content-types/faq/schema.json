{
  "kind": "singleType",
  "collectionName": "faq_page",
  "info": {
    "singularName": "faq",
    "pluralName": "faqs",
    "displayName": "FAQ",
    "description": "FAQ page content and questions"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": false
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "default": "FAQ'S"
    },
    "backgroundColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "faqItems": {
      "type": "component",
      "repeatable": true,
      "component": "blocks.faq-item"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "shared.seo"
    }
  }
}